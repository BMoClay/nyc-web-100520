<h1>Users#new</h1>

<%= form_for @user do |f| %>
  <%= f.label :user_name %>
  <%= f.text_field :user_name %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :age %>
  <%= f.text_field :age %>

  <%= f.label :cats_owned %>
  <%= f.text_field :cats_owned %>

  <%= f.submit %>
<% end %>
